{%  if tabMetricsProject!=null %}
<div>

    <h3><i class="bi bi-bar-chart"></i> Metrics Table for project : {{ projectName }}</h3>
<hr/>

<table class="dataTable table  border border-dark" id="TableMetricsProject">

    <thead>
    <tr>
        {#        <th>Resource Id</th>#}
        <th>Start</th>
        <th>End</th>
        <th>Value</th>
        <th>Unit</th>
        <th>Metric Type</th>
        <th>Metric Description</th>
        <th>Provider</th>
        <th>Install</th>
    </tr>
    </thead>

    <tbody>
    {%  for id, data in tabMetricsProject %}
        <tr>
            {#            <td class="name">{{ data.resourceId }} </td>#}
            <td class="start">{{ data.time_period_start }} </td>
            <td class="end">{{ data.time_period_end }} </td>
            <td class="value">{{ data.value }}</td>
            <td class="unit">{{ data.unit_type }}</td>
            <td>{{ data.metric_type }}</td>
            <td>{{ data.metric_description }}</td>
            <td class="provider">{{ data.provider }}</td>
{#            <td class="install">{{ data.installation }}</td>#}
        </tr>

    {% endfor %}

    </tbody>

</table>

</div>

{% endif %}

{%  if tabMetricsProvider!=null %}
    <div>

        <h3><i class="bi bi-bar-chart"></i> Metrics Table for provider {{ provider }}</h3>
        <hr/>

        <table class="dataTable table  border border-dark" id="TableMetricsProvider">

            <thead>
            <tr>
                {#        <th>Resource Id</th>#}
                <th>Start</th>
                <th>End</th>
                <th>Value</th>
                <th>Provider</th>
                <th>Install</th>
            </tr>
            </thead>

            <tbody>
            {%  for id, data in tabMetricsProvider %}
                <tr>
                    {#            <td class="name">{{ data.resourceId }} </td>#}
                    <td class="start">{{ data.time_period_start }} </td>
                    <td class="end">{{ data.time_period_end }} </td>
                    <td class="value">{{ data.value }}</td>
                    <td class="provider">{{ data.provider }}</td>
{#                    <td class="install">{{ data.installation }}</td>#}
                </tr>

            {% endfor %}

            </tbody>

        </table>

    </div>

{% endif %}